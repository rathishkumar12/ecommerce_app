<p id="notice"><%= notice %></p>

<h1>Seller Addresses</h1>

<table>
  <thead>
    <tr>
      <th>Seller</th>
      <th>Street name</th>
      <th>Lat</th>
      <th>Lon</th>
      <th>Pincode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seller_addresses.each do |seller_address| %>
      <tr>
        <td><%= seller_address.seller_id %></td>
        <td><%= seller_address.street_name %></td>
        <td><%= seller_address.lat %></td>
        <td><%= seller_address.lon %></td>
        <td><%= seller_address.pincode_id %></td>
        <td><%= link_to 'Show', seller_address %></td>
        <td><%= link_to 'Edit', edit_seller_address_path(seller_address) %></td>
        <td><%= link_to 'Destroy', seller_address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seller Address', new_seller_address_path %>
