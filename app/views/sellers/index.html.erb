<p id="notice"><%= notice %></p>

<h1>Sellers</h1>

<table>
  <thead>
    <tr>
      <th>Seller name</th>
      <th>Seller company name</th>
      <th>Email</th>
      <th>Phone number</th>
      <th>Password</th>
      <th>Is active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sellers.each do |seller| %>
      <tr>
        <td><%= seller.seller_name %></td>
        <td><%= seller.seller_company_name %></td>
        <td><%= seller.email %></td>
        <td><%= seller.phone_number %></td>
        <td><%= seller.password %></td>
        <td><%= seller.is_active %></td>
        <td><%= link_to 'Show', seller %></td>
        <td><%= link_to 'Edit', edit_seller_path(seller) %></td>
        <td><%= link_to 'Destroy', seller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seller', new_seller_path %>
