<br><h3 class="text-center fw-bold">Products</h3>
<br>
<table class="table">
  <thead>
    <tr >
      <th>Name</th>
      <th>Description</th>
      <th>Category</th>
      <th>Image url</th>
      <th>Seller</th>
      <th>Price</th>
      <th>Quantity</th>
      <th>Is active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% obj.each do |product| %>
      <tr scope="row" >
        <td><%= product.name %></td>
        <td><%= product.description %></td>
        <td><%= product.category_id %></td>
        <td><%= product.image_url %></td>
        <td><%= product.seller_id %></td>
        <td><%= product.price %></td>
        <td><%= product.quantity %></td>
        <td><%= product.is_active %></td>
        <td><%= link_to 'Show', product %></td>
        <td><%= link_to 'Edit', edit_product_path(product) %></td>
        <td><%= link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product', new_product_path(:seller_id =>params[:id]) %>